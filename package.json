  "version": "0.0.1",
  "author": "Hassan Mahmud",
  "dependencies": {
    "gatsby": "^2.27.4",
    "gatsby-image": "^2.6.0",
    "gatsby-plugin-manifest": "2.7.0",
    "gatsby-plugin-netlify-cms": "^4.5.0",
    "gatsby-plugin-offline": "3.2.9",
    "gatsby-plugin-postcss": "^3.2.0",
    "gatsby-plugin-react-helmet": "3.3.4",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.9.0",
    "gatsby-remark-copy-linked-files": "^2.5.0",
    "gatsby-remark-images": "^3.6.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^2.6.1",
    "gatsby-transformer-remark": "^2.11.1",
    "gatsby-transformer-sharp": "^2.7.0",
    "netlify-cms-app": "^2.14.1",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "start:cms": "npx netlify-cms-proxy-server",
    "start": "npm run clean && npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "0.2.1",
    "autoprefixer": "9.8.0",
    "babel-eslint": "10.1.0",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "eslint-loader": "3.0.4",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-react": "7.20.0",
    "gatsby-plugin-eslint": "2.0.8",
    "postcss": "^8.1.10",
    "prettier": "2.0.5",
    "tailwindcss": "2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind"
  },
  "bugs": {
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind/issues"
  }
